.dashboard__container {
    display: flex;
    padding: 0 160px;
    padding-top: 64px;
}

.dashboard__left_container {
    width: 50%;
}

.dashboard__right_container {
    width: 50%;
}

.dashboard__title {
    font-style: normal;
    font-weight: 500;
    font-size: 36px;
    line-height: 47px;
    color: #F4EDE8;
}

.dashboard__subtitle {
    margin-top: 12px;
    font-style: normal;
    font-weight: 500;
    font-size: 16px;
    line-height: 21px;
    color: #FF9000;
}

.dashboard__proximo_agendamento {
    font-style: normal;
    font-weight: 400;
    font-size: 20px;
    line-height: 26px;
    color: #999591;

    margin-top: 64px;
    margin-bottom: 16px;
}

.dashboard__sem_agendamento {
    font-size: 16px;
    line-height: 21px;
    color: #F4EDE8;
}