.container__agendamento {
  display: grid;
  grid-template-columns: 1fr;
  justify-content: center;
  align-content: flex-start;
  height: 100vh;
  width: 500px;
  margin: 50px auto;
}

.container__agendamento__title {
  color: #fff;
  margin: 10px 0;
}

.container__agendamento__title strong {
  color: #FF9000;
  font-size: 1.2rem;
}

.container__agendamento__warning {
  color: #fff;
  font-size: 1.2rem;
  margin: 10px 0;
}

.container__agendamento__btn button {
  width: 340px;
  height: 56px;
  background: #FF9000;
  border-radius: 10px;
  margin: 10px 0;
  cursor: pointer;
}